<h1>Meet the Brio Experience Provider Network!</h1>

<table>
  <tr>
    <th></th>
    <th>First Name</th>
    <th>Last Name</th>
    <th>City</th>
    <th>State</th>
    <th></th>
  </tr>

  <% @users.each do |user| %>
    <tr>
      <td>
        <img src="<%= user.photo %>" width="200px">
      </td>
      
      <td>
        <%= user.first_name %>
      </td>
      
      <td>
        <%= user.last_name %>
      </td>
      
      <td>
        <%= user.city %>
      </td>
      
    <td>
        <%= user.state %>
      </td>

      <td>
        <a href="/users/<%= user.id %>">
          Show profile
        </a>
      </td>
    </tr>
  <% end %>
</table>
